#include<stdio.h>
int main(){
    int n;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++)
        scanf("%d",&arr[i]);
    //test
    //for(int i=0;i<n;i++)
       // printf("%d ",arr[i]);
    int t;
    scanf("%d",&t);
    int arr1[t][2];
    for(int i=0;i<t;i++)
        for(int j=0;j<2;j++)
        scanf("%d",&arr1[i][j]);

    //for(int i=0;i<t;i++){
      //  for(int j=0;j<2;j++)
        //printf("%d ",arr1[i][j]);
        //printf("\n");
    //}
    for(int i=0;i<t;i++){
       int min = 501;
        for(int j=arr1[i][0];j<=arr1[i][1];j++){
            if(arr[j-1]<min)
                min = arr[j-1];

}
   printf("%d\n",min);
}




                
